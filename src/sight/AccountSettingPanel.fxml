<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane fx:controller="sight.AccountSettingPanelController" prefHeight="540.0" prefWidth="890.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<Label fx:id="HeadingLabel" layoutX="10.0" layoutY="10.0" text="Account Setting - " >
			<font><Font name="System Bold" size="16.0" /></font>
		</Label>
		<Label layoutX="10.0" layoutY="40.0" text="Password Change" underline="true">
			<font><Font name="System Bold" size="14.0" /></font>
		</Label>
		<Label layoutX="10.0" layoutY="70.0" text="Old Password" />	
		<Label layoutX="10.0" layoutY="100.0" text="New Password" />
		<Label layoutX="10.0" layoutY="130.0" text="Retype New Password" />
		<PasswordField fx:id="pfOldPassword" layoutX="150.0" layoutY="70.0" />
		<PasswordField fx:id="pfNewPassword" layoutX="150.0" layoutY="100.0" />
		<PasswordField fx:id="pfConfirmPassword" layoutX="150.0" layoutY="130.0" />
		<Button fx:id="btnSubmit" layoutX="10.0" layoutY="160.0" mnemonicParsing="false" onAction="#btnSubmitPressed" text="Submit" />
	</children>
</Pane>