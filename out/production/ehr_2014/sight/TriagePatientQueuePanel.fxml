<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane fx:controller="sight.TriagePatientQueuePanelController" prefHeight="580.0" prefWidth="890.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<Label fx:id="lPatientNum" layoutX="10.0" layoutY="10.0" prefHeight="20.0" prefWidth="200.0" text="" />
		<Button fx:id="btnStartNewPatient" mnemonicParsing="false" onAction="#btnStartNewPatientPressed" text="New Patient" layoutX="210.0" layoutY="10.0" prefHeight="20.0" prefWidth="120.0" />
		<Label layoutX="10.0" layoutY="40.0" prefHeight="20.0" prefWidth="120.0" text="Today's Patient:" />
		<Button fx:id="btnViewPatient" mnemonicParsing="false" onAction="#btnViewPatientPressed" text="View" layoutX="670.0" layoutY="40.0" prefHeight="20.0" prefWidth="100.0" />
		<Button fx:id="btnEditPatient" mnemonicParsing="false" onAction="#btnEditPatientPressed" text="Edit" layoutX="780.0" layoutY="40.0" prefHeight="20.0" prefWidth="100.0" />
		<TableView fx:id="tvTodayPatientTable" layoutX="10.0" layoutY="70.0" prefHeight="500.0" prefWidth="870.0" >
			<columns>
				<TableColumn fx:id="tbTagNumber" prefWidth="100.0" text="Tag Number" />
				<TableColumn fx:id="tbPatientName" prefWidth="248.0" text="Patient Name" />
				<TableColumn fx:id="tbTime" prefWidth="100.0" text="Time" />
				<TableColumn fx:id="tbNurse" prefWidth="140.0" text="Nurse" />
				<TableColumn fx:id="tbDoctor" prefWidth="140.0" text="Doctor" />
				<TableColumn fx:id="tbPharmacist" prefWidth="140.0" text="Pharmacist" />
			</columns>
		</TableView>
	</children>
</Pane>
