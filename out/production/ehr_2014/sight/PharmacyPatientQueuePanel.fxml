<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane fx:controller="sight.PharmacyPatientQueuePanelController" prefHeight="610.0" prefWidth="890.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<Label layoutX="10.0" layoutY="10.0" prefHeight="20.0" prefWidth="120.0" text="Patient's Queue:" />
		<Button fx:id="btnStartPatient" mnemonicParsing="false" onAction="#btnStartPatientPressed" text="Start" layoutX="780.0" layoutY="10.0" prefHeight="20.0" prefWidth="100.0" />
		<TableView fx:id="tvPatientQueueTable" layoutX="10.0" layoutY="40.0" prefHeight="260.0" prefWidth="870.0" >
			<columns>
				<TableColumn fx:id="tbTagNumber" prefWidth="100.0" text="Tag Number" />
				<TableColumn fx:id="tbPatientName" prefWidth="250.0" text="Patient Name" />
				<TableColumn fx:id="tbTime" prefWidth="100.0" text="Time" />
				<TableColumn fx:id="tbNurse" prefWidth="140.0" text="Nurse" />
				<TableColumn fx:id="tbDoctor" prefWidth="140.0" text="Doctor" />
			</columns>
		</TableView>
		<Label layoutX="10.0" layoutY="310.0" prefHeight="20.0" prefWidth="120.0" text="Today's Patient:" />
		<Button fx:id="btnViewPatient" mnemonicParsing="false" onAction="#btnViewPatientPressed" text="View" layoutX="780.0" layoutY="310.0" prefHeight="20.0" prefWidth="100.0" />
		<TableView fx:id="tvTodayPatientTable" layoutX="10.0" layoutY="340.0" prefHeight="270.0" prefWidth="870.0" >
			<columns>
				<TableColumn fx:id="tbTagNumber2" prefWidth="100.0" text="Tag Number" />
				<TableColumn fx:id="tbPatientName2" prefWidth="250.0" text="Patient Name" />
				<TableColumn fx:id="tbTime2" prefWidth="100.0" text="Time" />
				<TableColumn fx:id="tbNurse2" prefWidth="140.0" text="Nurse" />
				<TableColumn fx:id="tbDoctor2" prefWidth="140.0" text="Doctor" />
				<TableColumn fx:id="tbPharmacist" prefWidth="140.0" text="Pharmacist" />
			</columns>
		</TableView>
	</children>
</Pane>
